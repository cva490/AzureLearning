{
	"name": "OnpremisetoAzureBlob",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "dsonpremise",
						"type": "DatasetReference"
					},
					"name": "OnpremiseSQLSource"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "dsLanding",
						"type": "DatasetReference"
					},
					"name": "BlobStorage"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     limit: 100,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> OnpremiseSQLSource",
				"OnpremiseSQLSource sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> BlobStorage"
			]
		}
	}
}